{
  "name": "test-store-cypress",
  "version": "1.0.0",
  "description": "automation test store testing with cypress",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run --headless",
    "triggerAllTests-chrome-headed": "npx cypress run --browser chrome --headed",
    "triggerrAllTests-dashboard": "npx cypress run --record --key 74412f26-1225-473c-979e-25d6e52efe25",
    "triggerHomePageTests": "npx cypress run --spec cypress/e2e/homePageTests.js",
    "triggerAuthTests": "npx cypress run --spec cypress/e2e/authTests.js",
    "triggerAccountTests": "npx cypress run --spec cypress/e2e/accountTests.js",
    "triggerProductTests": "npx cypress run --spec cypress/e2e/productTests.js",
    "triggerShoppingTests": "npx cypress run --spec cypress/e2e/shoppingTests-*.js",
    "triggerHomePageTests-dashboard": "npx cypress run --spec cypress/e2e/homePageTests.js --record --key 74412f26-1225-473c-979e-25d6e52efe25",
    "triggerAuthTests-dashboard": "npx cypress run --spec cypress/e2e/authTests.js --record --key 74412f26-1225-473c-979e-25d6e52efe25",
    "triggerAccountTests-dashboard": "npx cypress run --spec cypress/e2e/accountTests.js --record --key 74412f26-1225-473c-979e-25d6e52efe25",
    "triggerProductTests-dashboard": "npx cypress run --spec cypress/e2e/productTests.js --record --key 74412f26-1225-473c-979e-25d6e52efe25",
    "triggerShoppingTests-dashboard": "npx cypress run --spec cypress/e2e/shoppingTests-*.js --record --key 74412f26-1225-473c-979e-25d6e52efe25"
  },
  "author": "czajkowski witkor",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.17.2"
  }
}
